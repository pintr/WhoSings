{
    "expo": {
        "name": "Who Sings",
        "description": "Quiz app for identifying the singer of a certain song by lyrics.",
        "slug": "WhoSings",
        "privacy": "public",
        "sdkVersion": "26.0.0",
        "platforms": ["ios", "android"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        }
    }
}
